<% if (pn.pnTotal > 1) { %>
  <div class="pagination">
    <div class="prev">
      <% if (pn.pnNum > pn.pnSize) { %>
        <a href="?<% if (typeof (searchType) !== 'undefined' && searchType) { %>searchType=<%= searchType %>&<% } %>&<% if (typeof (keyword) !== 'undefined' && keyword) { %>keyword=<%= keyword %>&<% } %><% if (typeof (category) !== 'undefined' && category) { %>category=<%= category %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.city) !== 'undefined' && location.city) { %>city=<%= location.city %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.province) !== 'undefined' && location.province) { %>province=<%= location.province %>&<% } %><% if (typeof (filter) !== 'undefined' && filter) { %>filter=<%= filter %>&<% } %><% if (typeof (align) !== 'undefined' && align) { %>align=<%= align %>&<% } %>page=<%= pn.pnStart - pn.pnSize %>">이전</a>
      <% } %>
    </div>
    <div class="number">
      <% for (let i = pn.pnStart; i < pn.pnEnd + 1; i++) { %>
        <% if (pn.pnNum === i) { %>
          <a class="selected"><%= i %></a>
        <% } else { %>
          <a class="notSelected" href="?<% if (typeof (searchType) !== 'undefined' && searchType) { %>searchType=<%= searchType %>&<% } %><% if (typeof (keyword) !== 'undefined' && keyword) { %>keyword=<%= keyword %>&<% } %><% if (typeof (category) !== 'undefined' && category) { %>category=<%= category %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.city) !== 'undefined' && location.city) { %>city=<%= location.city %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.province) !== 'undefined' && location.province) { %>province=<%= location.province %>&<% } %><% if (typeof (filter) !== 'undefined' && filter) { %>filter=<%= filter %>&<% } %><% if (typeof (align) !== 'undefined' && align) { %>align=<%= align %>&<% } %>page=<%= i %>"><%= i %></a>
        <% } %>
      <% } %>
    </div>
    <div class="next">
      <% if ((pn.pnTotal - pn.pnStart) > pn.pnSize) { %>
        <a href="?<% if (typeof (searchType) !== 'undefined' && searchType) { %>searchType=<%= searchType %>&<% } %>&<% if (typeof (keyword) !== 'undefined' && keyword) { %>keyword=<%= keyword %>&<% } %><% if (typeof (category) !== 'undefined' && category) { %>category=<%= category %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.city) !== 'undefined' && location.city) { %>city=<%= location.city %>&<% } %><% if (typeof (location) !== 'undefined' && location && typeof (location.province) !== 'undefined' && location.province) { %>province=<%= location.province %>&<% } %><% if (typeof (filter) !== 'undefined' && filter) { %>filter=<%= filter %>&<% } %><% if (typeof (align) !== 'undefined' && align) { %>align=<%= align %>&<% } %>page=<%= pn.pnStart + pn.pnSize %>">다음</a>
      <% } %>
    </div>
  </div>
<% } %>